DROP DATABASE IF EXISTS liqui_escuela;
CREATE DATABASE liqui_escuela;
USE liqui_escuela;

DROP DATABASE IF EXISTS liqui_escuela;
CREATE DATABASE liqui_escuela;
USE liqui_escuela;

CREATE TABLE escuela(
        id_escuela INT NOT NULL AUTO_INCREMENT
,       nombre VARCHAR(200)
,		numero INT UNIQUE
,		categoria VARCHAR(200)
,		ciudad VARCHAR(200)
,		nivel VARCHAR(200)
);
